    <TEXTURE id="1">
      <file path="images/1.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="2">
      <file path="images/2.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="3">
      <file path="images/3.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="4">
      <file path="images/4.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="5">
      <file path="images/5.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="6">
      <file path="images/6.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="7">
      <file path="images/7.png" />
