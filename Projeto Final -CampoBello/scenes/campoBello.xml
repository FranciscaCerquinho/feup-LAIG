      <TRANSLATION x="0.5" y="0.5" z="0.5" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="footer2">
      <MATERIAL id="null" />
      <TEXTURE id="wood" />

      <SCALE sx="29" sy="1" sz="0.2" />
      <TRANSLATION x="0.5" y="0.5" z="0.5" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="leg">
